{
  "entrypoint": "index.html",
  "shell": "src/cdn-app.html",
  "fragments": [
    "src/home-view.html",
    "src/docs-view.html",
    "src/libs-view.html",
    "src/about-view.html",
    "src/status-view.html",
    "src/404-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es5-bundled"
    }
  ]
}
